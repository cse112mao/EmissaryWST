{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://emissaryws.herokuapp.com/login"
    },
    {
      "type": "verifyElementPresent",
      "locator": {
        "type": "css selector",
        "value": "img"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Dear user, log in to access the admin area"
    },
    {
      "type": "verifyElementValue",
      "locator": {
        "type": "id",
        "value": "username"
      },
      "value": ""
    },
    {
      "type": "verifyTextPresent",
      "text": ""
    },
    {
      "type": "verifyTextPresent",
      "text": "Login In"
    },
    {
      "type": "verifyTextPresent",
      "text": "Forgot password"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "username"
      },
      "text": "tester@tester.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "password"
      },
      "text": "1234"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "loginButton"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Dashboard"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Dashboard"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}